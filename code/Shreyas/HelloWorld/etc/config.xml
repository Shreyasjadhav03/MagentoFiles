<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <social_section>
            <profile_group>
                <enable_field>1</enable_field>
                <facebook_field>https://www.facebook.com/</facebook_field>
                <instagram_field>https://www.instagram.com/</instagram_field>
                <twitter_field>https://www.twitter.com/</twitter_field>
            </profile_group>
        </social_section>
    </default>
    <default>
        <carriers>
            <simpleshipping>
                <active>1</active>
                <sallowspecific>0</sallowspecific>
                <model>Shreyas\HelloWorld\Model\Carrier\Shipping</model>
                <name>Mageplaza Sample Shipping Method</name>
                <price>10.00</price>
                <title>Mageplaza Sample Shipping Method</title>
                <specificerrmsg>This shipping method is not available. To use this shipping method, please contact us.</specificerrmsg>
                <handling_type>F</handling_type>
            </simpleshipping>
        </carriers>
    </default>
</config>